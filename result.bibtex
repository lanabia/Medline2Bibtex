@Article{2011125,
author  = {Silverberg, N. and Ryan, L. and Carrillo, M. and Sperling, R. and Petersen, R. and Posner, H. and Snyder, P. and Hilsabeck, R. and Gallagher, M. and Raber, J. and Rizzo, A. and Possin, K. and King, J. and Kaye, J. and Ott, B. and Albert, M. and Wagster, M. and Schinka, J. and Cullum, C. and Farias, S. and Balota, D. and Rao, S. and Loewenstein, D. and Budson, A. and Brandt, J. and Manly, J. and Barnes, L. and Strutt, A. and Gollan, T. and Ganguli, M. and Babcock, D. and Litvan, I. and Kramer, J. and Ferman, T.},
title  = {Assessment of cognition in early dementia.},
journal  = {Alzheimer's & dementia : the journal of the Alzheimer's Association},
volume  = {7},
number  = {3},
pages  = {e60-76},
year  = {2011},
abstract  = {Better tools for assessing cognitive impairment in the early stages of Alzheimer‚Äôs disease (AD) are required to enable diagnosis of the disease before substantial neurodegeneration has taken place and to allow detection of subtle changes in the early stages of progression of the disease. The National Institute on Aging and the Alzheimer‚Äôs Association convened a meeting to discuss state of the art methods for cognitive assessment, including computerized batteries, as well as new approaches in the pipeline. Speakers described research using novel tests of object recognition, spatial navigation, attentional control, semantic memory, semantic interference, prospective memory, false memory and executive function as among the tools that could provide earlier identification of individuals with AD. In addition to early detection, there is a need for assessments that reflect real-world situations in order to better assess functional disability. It is especially important to develop assessment tools that are useful in ethnically, culturally and linguistically diverse populations  as well as in individuals with neurodegenerative disease other than AD.},
address  = {Division of Neuroscience, National Institute on Aging, National Institutes of Health, Bethesda MD Division of Neuroscience, National Institute on Aging, National Institutes of Health, Bethesda MD Alzheimer‚Äôs Association, Chicago, IL Center for Alzheimer Research and Treatment, Department of Neurology, Brigham and Women‚Äôs Hospital, Massachusetts General Hospital, Harvard Medical School, Boston, MA Department of Neurology, Mayo Clinic, Rochester, MN Rhode Island Hospital and Department of Neurology, Alpert Medical School of Brown University, Providence, RI Department of Psychiatry, University of Texas Health Science Center at San Antonio and Psychology Service, South Texas Veterans Health Care System Department of Psychological and Brain Sciences, Johns Hopkins University, Baltimore, MD Department of Behavioral Neuroscience and Neurology and Division of Neuroscience, ONPRC, Oregon Health and Science University University of Southern California, Institute for Creative Technologies, Playa Vista, CA Department of Neurology, University of California, San Francisco, CA Division of Behavioral and Social Research, National Institute on Aging, National Institutes of Health, Bethesda MD Departments of Neurology and Biomedical Engineering, Layton Aging & Alzheimer‚Äôs Disease Center, Oregon Center for Aging & Technology, Oregon Health & Science University and Portland Veteran‚Äôs Affairs Medical Center, Portland, OR Department of Cognitive, Linguistic and Psychological Sciences, Brown University, Street, Providence, RI Department of Neurology, Johns Hopkins University School of Medicine, Baltimore,  MD Division of Neuroscience, National Institute on Aging, National Institutes of Health, Bethesda MD James A. Haley VA Medical Center and Department of Psychiatry, University of South Florida, Tampa, FL University of Texas Southwestern Medical Center at Dallas, TX Department of Neurology, University of California, Davis, Sacramento, California Department of Psychology, Washington University, St. Louis, Missouri Schey Center for Cognitive Neuroimaging, Neurological Institute, Cleveland Clinic, Cleveland, OH Wien Center for Alzheimer's Disease and Memory Disorders, Mount Sinai Medical Center, Miami Beach, FL Department of Neurology, Boston University Alzheimer‚Äôs Disease Center, Boston University School of Medicine, Bedford, MA and Center for Translational Cognitive Neuroscience, Geriatric Research Education Clinical Center, Edith Nourse Rogers Memorial Veterans Hospital, Bedford, MA Department of Psychiatry and Behavioral Sciences, The Johns Hopkins University School of Medicine, Baltimore, MD Taub Institute for Research on Alzheimer‚Äôs Disease and the Aging Brain, Columbia  University Medical Center, New York, NY Rush Alzheimer‚Äôs Disease Center, Department of Neurological Sciences, and Department of Behavioral Sciences, 4Rush Institute for Healthy Aging, Rush University Medical Center, Chicago, IL Baylor College of Medicine, Houston, TX Department of Psychiatry, University of California, San Diego, CA Departments of Psychiatry and Neurology, School of Medicine, University of Pittsburgh; Department of Epidemiology, Graduate School of Public Health, University of Pittsburgh, Pittsburgh, PA National Institute of Neurological Disorders and Stroke, National Institutes of Health, Bethesda, MD Division of Movement Disorders, Department of Neurology, University of Louisville, Louisville, KY Department of Neurology, University of California, San Francisco, CA Department of Psychiatry and Psychology, Mayo Clinic, College of Medicine, Jacksonville, FL},
