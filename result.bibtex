@Article{2012411,
author  = {MacLachlan, R. and Becker, B. and Tabar√©s, J. and Podnar, G. and Lobes, L. and Riviere, C.},
title  = {Micron: an Actively Stabilized Handheld Tool for Microsurgery.},
journal  = {Society},
volume  = {28},
number  = {1},
pages  = {195-212},
year  = {2012},
abstract  = {We describe the design and performance of a hand-held actively stabilized tool to increase accuracy in micro-surgery or other precision manipulation. It removes involuntary motion such as tremor by actuating the tip to counteract the effect of the undesired handle motion. The key components are a three-degree-of-freedom  piezoelectric manipulator that has 400 Œºm range of motion, 1 N force capability,  and bandwidth over 100 Hz, and an optical position measurement subsystem that acquires the tool pose with 4 Œºm resolution at 2000 samples/s. A control system using these components attenuates hand motion by at least 15 dB (a fivefold reduction). By considering the effect of the frequency response of Micron on the  human visual feedback loop, we have developed a filter that reduces unintentional motion, yet preserves intuitive eye-hand coordination. We evaluated the effectiveness of Micron by measuring the accuracy of the human/machine system in  three simple manipulation tasks. Handheld testing by three eye surgeons and three non-surgeons showed a reduction in position error of between 32% and 52%, depending on the error metric.},
address  = {Robotics Institute, Carnegie Mellon University, Pittsburgh, PA 15213 USA Robotics Institute, Carnegie Mellon University, Pittsburgh, PA 15213 USA University of Valladolid, Spain Robotics Institute, Carnegie Mellon University, Pittsburgh, PA 15213 USA Dept. of Ophthalmology, University of Pittsburgh Medical Center, Pittsburgh, PA 15213 USA Robotics Institute, Carnegie Mellon University, Pittsburgh, PA 15213 USA},
note  = {doi:10.1109/TRO.2011.2169634.}
}

